{
    "name": "laravel/lumen-breeze",
    "description": "Minimal Lumen authentication scaffolding with vue and tailwindcss",
    "keywords": ["laravel", "auth"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/pu-explore/lumen-breeze/issues",
        "source": "https://github.com/pu-explore/lumen-breeze"
    },
    "authors": [
        {
            "name": "xiaozhi",
            "email": "xiaozhi2015@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/lumen-framework": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Lumen\\Breeze\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lumen\\Breeze\\BreezeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
